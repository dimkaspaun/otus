#Запуск vagrant box
#1.	Скачать файл box https://app.vagrantup.com/centos/boxes/7
#2.	Добавить образ 
#vagrant box add --name 'centos/7' ~/test_vm/CentOS-7-x86_64-Vagrant-2004_01.VirtualBox.box
#3.	Создать Vagrantfile
#4.	Выполнить файл vagrant up


Vagrant.configure("2") do |config|
  config.vm.box = 'centos/7'   

end